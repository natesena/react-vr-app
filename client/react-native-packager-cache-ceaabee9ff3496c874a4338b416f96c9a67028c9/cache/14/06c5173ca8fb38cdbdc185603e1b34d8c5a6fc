 * LICENSE file in the root directory of this source tree. An additional grant
 * of patent rights can be found in the PATENTS file in the same directory.
 *
 * @providesModule VideoUtils
 */function videoTimeFormat(time){if(!time){return'--:--';}var timeS=Math.floor(time);var seconds=timeS%60;var minutes=Math.floor(timeS/60)%60;var h=Math.floor(timeS/3600);// Mandate two digit minutes only if h is non-zero
var mm=h?("0"+minutes).slice(-2):minutes;
var ss=("0"+seconds).slice(-2);
var timertext=h?h+":"+mm+":"+ss:mm+":"+ss;
return timertext;
}